{
  "name": "tshafer/ownable",
  "description": "Easy ownership for Eloquent Models on Laravel 5",
  "license": "MIT",
  "keywords": [
    "kenarkose",
    "ownable",
    "ownership",
    "eloquent",
    "laravel",
    "laravel5"
  ],
  "version": "1.1.2",
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Tshafer\\Ownable\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "suggest": {
    "laravel/framework": "This package requires Laravel 5."
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2-dev"
    }
  }
}
